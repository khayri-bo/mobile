<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_frag"
    app:startDestination="@id/choise_admin_client_Fragment">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.reclamation.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.example.reclamation.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" >
        <action
            android:id="@+id/action_resultFragment_to_choise_admin_client_Fragment"
            app:destination="@id/choise_admin_client_Fragment" />
        <action
            android:id="@+id/action_resultFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/choise_admin_client_Fragment"
        android:name="com.example.reclamation.Choise_admin_client_Fragment"
        android:label="fragment_choise_admin_client_"
        tools:layout="@layout/fragment_choise_admin_client_" >
        <action
            android:id="@+id/to_admin_action"
            app:destination="@id/resultFragment" />
        <action
            android:id="@+id/action_choise_admin_client_Fragment_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/reclamer_action"
            app:destination="@id/reclamerFragment" />
    </fragment>
    <fragment
        android:id="@+id/reclamerFragment"
        android:name="com.example.reclamation.reclamerFragment"
        android:label="fragment_reclamer"
        tools:layout="@layout/fragment_reclamer" >
        <action
            android:id="@+id/action_reclamerFragment_to_choise_admin_client_Fragment"
            app:destination="@id/choise_admin_client_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.reclamation.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" >
        <action
            android:id="@+id/action_detailFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>
</navigation>